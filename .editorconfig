dotnet_diagnostic.SA1618.severity = none

# SA1602: Enumeration items should be documented
dotnet_diagnostic.SA1602.severity = none

# CA1305: Specify IFormatProvider
dotnet_diagnostic.CA1305.severity = none

# IDE0045: Convert to conditional expression
dotnet_diagnostic.IDE0045.severity = silent

# IDE0039: Use local function
dotnet_diagnostic.IDE0039.severity = silent

# CA1304: Specify CultureInfo
dotnet_diagnostic.CA1304.severity = none

# CA1310: Specify StringComparison for correctness
dotnet_diagnostic.CA1310.severity = silent

# SA1623: Property summary documentation should match accessors
dotnet_diagnostic.SA1623.severity = none

# CA1311: Specify a culture or use an invariant version
dotnet_diagnostic.CA1311.severity = none

# IDE0270: Null check can be simplified
dotnet_diagnostic.IDE0270.severity = silent

# IDE0280: Use 'nameof'
dotnet_diagnostic.IDE0280.severity = silent

# IDE0072: This rule concerns specifying all the missing cases for a switch expression
dotnet_diagnostic.IDE0072.severity = silent

# CA1859: Use concrete types when possible for improved performance
dotnet_diagnostic.CA1859.severity = silent

# CA1860: Avoid using 'Enumerable.Any()' extension method
dotnet_diagnostic.CA1860.severity = none

# ASP0018: Unused route parameter
dotnet_diagnostic.ASP0018.severity = silent

# CA1862: Use the 'StringComparison' method overloads to perform case-insensitive string comparisons
dotnet_diagnostic.CA1862.severity = silent

# IDE0305: Collection initialization can be simplified
dotnet_diagnostic.IDE0305.severity = silent

# SA1117: Parameters should be on same line or separate lines
dotnet_diagnostic.SA1117.severity = silent

# CA2208: Instantiate argument exceptions correctly
dotnet_diagnostic.CA2208.severity = suggestion

# CA1711: Identifiers should not have incorrect suffix
dotnet_diagnostic.CA1711.severity = silent

# SA1204: Static elements should appear before instance elements
dotnet_diagnostic.SA1204.severity = none

# CA1308: Normalize strings to uppercase
dotnet_diagnostic.CA1308.severity = none

# CA1062: Validate arguments of public methods
dotnet_diagnostic.CA1062.severity = none

# CA1054: URI-like parameters should not be strings
dotnet_diagnostic.CA1054.severity = none

# CA1055: URI-like return values should not be strings
dotnet_diagnostic.CA1055.severity = silent

# CA1056: URI-like properties should not be strings
dotnet_diagnostic.CA1056.severity = none

# CA2225: Operator overloads have named alternates
dotnet_diagnostic.CA2225.severity = none

# SA1402: File may only contain a single type
dotnet_diagnostic.SA1402.severity = none

[*Test.cs]

# CA1707: Identifiers should not contain underscores
dotnet_diagnostic.CA1707.severity = none

# S2344: Enumeration type names should not have "Flags" or "Enum" suffixes
dotnet_diagnostic.S2344.severity = none

